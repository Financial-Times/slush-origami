{
	"description": "<%= componentDescription %>",
	"origamiType": "module",
	"origamiVersion": 1,
	"keywords": "<%= componentKeywords %>",
	"origamiCategory": "<%= componentType %>s",
	"support": "<%= issuesUrl %>",
	"supportStatus": "<%= supportStatus %>",
	"browserFeatures": {
		"required": <%= JSON.stringify(browserFeaturesRequired, null, '\t') %>,
		"optional": <%= JSON.stringify(browserFeaturesOptional, null, '\t') %>
	},
	"ci": {
		"circle": "<%= ciUrl %>"
	},
	"demosDefaults": {},
	"demos": [
		{
			"name": "demo",
			"js": "demos/src/demo.js",
			"template": "demos/src/demo.mustache",
			"description": "Basic demo"
		},
		{
			"name": "pa11y",
			"js": "demos/src/demo.js",
			"template": "demos/src/pa11y.mustache",
			"hidden": true,
			"description": "Accessibility test will be run against this demo"
		}
	]
}
